{
  "name": "@carevoyance/interview-project",
  "version": "1.0.0",
  "scripts": {
    "db:reset": "rimraf data.db && echo 'The database will be reset next time the server starts'",
    "dev:client": "snowpack dev",
    "dev:server": "API_PORT=${API_PORT:-5005} NODE_INSPECT=true NODE_ENV=development tsc-watch --noClear -p tsconfig.server.json --onSuccess 'node dist-server/index.js'",
    "dev": "API_PORT=${API_PORT:-5005} run-p dev:client dev:server",
    "build:client": "snowpack build",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "run-p build:client build:server",
    "eject:prepare": "node prepare.mjs",
    "eject:makedb": "node data/build-database.mjs --only-if-missing",
    "eject:mkdir": "mkdir -p",
    "eject:copy": "rsync -avr --exclude node_modules --exclude interview-project.build.log --exclude interview-project.build.error.log --exclude prepare.mjs --exclude data.db --exclude data/build-database.mjs --exclude dist-server --exclude build --exclude .rush .",
    "eject": "run-s eject:makedb 'eject:mkdir {1}' eject:prepare 'eject:copy {1}' --",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "~5.15.1",
    "@fortawesome/free-solid-svg-icons": "~5.15.1",
    "d3-array": "~2.8.0",
    "d3-color": "~2.0.0",
    "d3-delaunay": "~5.3.0",
    "d3-geo": "~2.0.1",
    "d3-interpolate": "~2.0.1",
    "d3-quadtree": "~2.0.0",
    "d3-scale-chromatic": "~2.0.0",
    "d3-scale": "~3.2.3",
    "d3-shape": "~2.0.0",
    "d3plus-shape": "~0.17.0",
    "d3plus-text": "~0.10.1",
    "dayjs": "~1.9.6",
    "fastify-plugin": "~3.0.0",
    "fastify": "~3.8.0",
    "just-clamp": "~3.1.0",
    "just-flatten-it": "~2.1.0",
    "just-group-by": "~1.0.0",
    "just-index": "~2.1.0",
    "just-range": "~2.1.0",
    "just-safe-get": "~2.0.0",
    "just-unique": "~3.2.0",
    "just-zip-it": "~2.1.0",
    "ky": "~0.25.0",
    "nanoid": "~3.1.18",
    "sorters": "~0.0.4",
    "source-map-support": "~0.5.19",
    "sqlite": "~4.0.17",
    "sqlite3": "~5.0.0",
    "svelte": "~3.30.1",
    "tippy.js": "~6.2.7",
    "uuid": "~8.3.1",
    "npm-run-all": "~4.1.5"
  },
  "devDependencies": {
    "layercake": "~4.0.3",
    "@microsoft/rush-lib": "~5.35.2",
    "@snowpack/app-scripts-svelte": "~1.9.2",
    "@snowpack/plugin-dotenv": "~2.0.4",
    "@snowpack/plugin-postcss": "~1.0.7",
    "@snowpack/plugin-svelte": "~2.2.0",
    "@snowpack/plugin-typescript": "~1.1.0",
    "@tailwindcss/forms": "~0.2.1",
    "@testing-library/jest-dom": "~5.11.6",
    "@testing-library/svelte": "^3.0.0",
    "@types/glob": "~7.1.3",
    "@types/node": "^14",
    "@types/rimraf": "~3.0.0",
    "@typescript-eslint/eslint-plugin": "~4.8.1",
    "@typescript-eslint/parser": "~4.8.1",
    "autoprefixer": "~10.0.2",
    "cssnano": "~4.1.10",
    "eslint-config-prettier": "~6.15.0",
    "eslint": "~7.13.0",
    "faker": "~5.1.0",
    "glob": "~7.1.6",
    "jest": "~26.6.3",
    "pg-promise": "~10.7.5",
    "postcss-cli": "~8.3.0",
    "postcss-import": "~13.0.0",
    "postcss-load-config": "^2.1.0",
    "postcss": "~8.1.7",
    "prettier": "~2.2.0",
    "rimraf": "~3.0.2",
    "snowpack": "~2.15",
    "svelte-preprocess": "~4.6.1",
    "tailwindcss": "~2.0.1",
    "tsc-watch": "~4.2.9",
    "typescript": "~4.1.2",
    "npm-run-all": "~4.1.5"
  }
}
